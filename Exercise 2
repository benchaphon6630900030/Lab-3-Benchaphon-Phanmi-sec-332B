print("\n","--push--")

Array = [ ]
print(Array)

Array.append("A")
print(Array)
Array.append("B")
print(Array)
Array.append("C")
print(Array)
Array.append("D")
print(Array)
Array.append("E")
print(Array)
Array.append("F")
print(Array)

print("\n","--reverse pop--")
for i in range(6):
    Array.append(Array.pop())
print("\n",Array)
